{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "Seed for React project",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "start": "npm run serve:dev",
    "eslint:fix": "eslint . --ext .js --fix",
    "eslint:log": "eslint . --ext .js",
    "pa11y": "pa11y --config ./pa11y-config.json",
    "precommit": "npm run eslint:fix && npm run tslint:fix && npm run type-check && npm run test",
    "sass-lint:log": "sass-lint --verbose",
    "serve:dev": "webpack-dev-server --env.NODE_ENV=development --progress --colors",
    "test": "jest --verbose --config ./test/jest.config.js",
    "tslint": "tslint",
    "tslint:fix": "tslint --fix --project tsconfig.json",
    "tslint:log": "tslint --project tsconfig.json",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alessioprestileo/react-seed.git"
  },
  "keywords": [
    "Babel",
    "fontend",
    "React",
    "TypeScript",
    "Webpack"
  ],
  "author": "Alessio Prestileo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.1",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "asciidoctor.js": "^1.5.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "empty": "^0.10.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.9",
